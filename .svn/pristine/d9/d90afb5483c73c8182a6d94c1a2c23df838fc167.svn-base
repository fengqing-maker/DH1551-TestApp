                      </item>          			
                </delivery_system_descriptor>        
          </transport_stream>      

          <transport_stream attr="other">        
                <!-- Transport Stream ID : [0x0000 ~ 0xFFFF] -->        
    	        <transport_stream_id> 0x0005 </transport_stream_id>        
    	        <!-- Original Network ID : [0x0000 ~ 0xFFFF] -->        
    	        <original_network_id> 0x5000 </original_network_id>        

    	        <delivery_system_descriptor>          
                      <!-- Network Type : [cable, satellite, terrestrial]. -->          
                      <network_type>satellite</network_type>          
                      <!-- This section would take effective only when the "network_type" is set to DVB-C -->          
                      <item type="cable">            
                            <!-- The frequency is a 32-bit field giving the 4-bit BCD values specifying 8 characters of the frequency value. -->            
                            <!-- For the cable_delivery_system_descriptor, the frequency is coded in MHz, where the decimal occurs after the fourth -->            
                            <!-- character (e.g. 0570.0000 MHz) -->            
                            <frequency> 0x05700000 </frequency>            
                            <!-- The FEC_outer is a 4-bit field specifying the outer Forward Error Correction (FEC) scheme used according to the table below. -->            
                            <!-- FEC_outer bit 3210  |  Description                     -->            
                            <!--    0x0              |  not defined                     -->            
                            <!--    0x1              |  no outer FEC coding             -->            
                            <!--    0x2              |  RS(204/188)                     -->            
                            <!--    0x3 ~ 0xF        |  reserved for future use         -->            
                            <FEC_outer> 0x0 </FEC_outer>            
                            <!-- This is an 8-bit field. It specifies the modulation scheme used on a cable delivery system according to the table below. -->            
                            <!-- Modulation (hex)    |  Description                     -->            
                            <!--    0x00             |  not defined                     -->            
                            <!--    0x01             |  16-QAM                          -->            
                            <!--    0x02             |  32-QAM                          -->            
                            <!--    0x03             |  64-QAM                          -->            
                            <!--    0x04             |  128-QAM                         -->            
                            <!--    0x05             |  256-QAM                         -->            
                            <!--    0x06 ~ 0xFF      |  reserved for future use         -->            
                            <modulation> 0x03 </modulation>            
                            <!-- The symbol_rate is a 28-bit field giving the 4-bit BCD values specifying 7 characters of the symbol_rate -->            
                            <!-- in Msymbol/s where the decimal point occurs after the third character (e.g. 006.8750). -->            
                            <symbol_rate> 0x0068750 </symbol_rate>            
                            <!-- The FEC_inner is a 4-bit field specifying the inner FEC scheme used according to the table below. -->            
                            <!-- FEC_inner bit 3210  |  Description                     -->            
                            <!--    0x0              |  not defined                     -->            
                            <!--    0x1              |  1/2 conv. code rate             -->            
                            <!--    0x2              |  2/3 conv. code rate             -->            
                            <!--    0x3              |  3/4 conv. code rate             -->            
                            <!--    0x4              |  5/6 conv. code rate             -->            
                            <!--    0x5              |  7/8 conv. code rate             -->            
                            <!--    0x6              |  8/9 conv. code rate             -->            
                            <!--    0x7              |  3/5 conv. code rate             -->            
                            <!--    0x8              |  4/5 conv. code rate             -->            
                            <!--    0x9              |  9/10 conv. code rate            -->            
                            <!--    0xA ~ 0xE        |  reserved for future use         -->            
                            <!--    0xF              |  no conv. Coding                 -->            
                            <FEC_inner> 0x0 </FEC_inner>            
                      </item>          
                      <!-- This section would take effective only when the "network_type" is set to DVB-S -->          
                      <item type="satellite">            
                            <!-- The frequency is a 32-bit field giving the 4-bit BCD values specifying 8 characters of the frequency value. -->            
                            <!-- For the satellite_delivery_system_descriptor the frequency is coded in GHz, where the decimal point occurs after the -->            
                            <!-- third character (e.g. 011.74000 GHz). -->            
                            <frequency> 0x0388000 </frequency>            
                            <!-- The orbital_position is a 16-bit field giving the 4-bit BCD values specifying 4 characters of the -->            
                            <!-- orbital position in degrees where the decimal point occurs after the third character (e.g. 156.0¡᧮ -->            
                            <orbital_position> 0x1560 </orbital_position>            
                            <!-- The west_east_flag is a 1-bit field indicating if the satellite position is in the western or eastern part of -->            
                            <!-- the orbit. A value "0" indicates the western position and a value "1" indicates the eastern position. -->            
                            <west_east_flag> 1 </west_east_flag>            
                            <!-- The polarization is a 2-bit field specifying the polarization of the transmitted signal. The first bit defines -->            
                            <!-- whether the polarization is linear or circular. See the table below. -->            
                            <!-- Polarization        |  Description                     -->            
                            <!--    0x0              |  linear - horizontal             -->            
                            <!--    0x1              |  linear - vertical               -->            
                            <!--    0x2              |  Circular - left                 -->            
                            <!--    0x3              |  Circular - right                -->            
                            <polarization> 0x0 </polarization>            
                            <!-- This 2 bit field specifies the modulation system factor used in DVB-S2. -->            
                            <!-- roll_off            |  Description                     -->            
                            <!--    0x0              |  ¦�= 0,35                        -->            
                            <!--    0x1              |  ¦�= 0,25                        -->            
                            <!--    0x2              |  ¦�= 0,20                        -->            
                            <!--    0x3              |  reserved                        -->            
                            <roll_off> 0x0 </roll_off>            
                            <!-- This is a 1-bit field. It specifies the broadcast scheme used on a satellite delivery system according to the table below. -->            
                            <!-- modulation_system   |  Description                     -->            
                            <!--    0                |  DVB-S                           -->            
                            <!--    1                |  DVB-S2                          -->            				
                            <modulation_system> 0 </modulation_system>            
                            <!-- This is a 2-bit field. It specifies the modulation scheme used on a satellite delivery system according to the table below. -->            
                            <!-- modulation type     |  Description                     -->            
                            <!--    0x0              |  Auto                            -->            
                            <!--    0x1              |  QPSK                            -->            
                            <!--    0x2              |  8PSK                            -->            
                            <!--    0x3              |  16-QAM (n/a for DVB-S2)         -->            
                            <modulation_type> 0x1 </modulation_type>            
                            <!-- The symbol_rate is a 28-bit field giving the 4-bit BCD values specifying 7 characters of the symbol_rate -->            
                            <!-- in Msymbol/s where the decimal point occurs after the third character (e.g. 026.0000). -->            
                            <symbol_rate> 0x0275000 </symbol_rate>            
                            <!-- The FEC_inner is a 4-bit field specifying the inner FEC scheme used according to the table below. -->            
                            <!-- FEC_inner bit 3210  |  Description                     -->            
                            <!--    0x0              |  not defined                     -->            
                            <!--    0x1              |  1/2 conv. code rate             -->            
                            <!--    0x2              |  2/3 conv. code rate             -->            
                            <!--    0x3              |  3/4 conv. code rate             -->            
                            <!--    0x4              |  5/6 conv. code rate             -->            
                            <!--    0x5              |  7/8 conv. code rate             -->            
                            <!--    0x6              |  8/9 conv. code rate             -->            
                            <!--    0x7              |  3/5 conv. code rate             -->            
                            <!--    0x8              |  4/5 conv. code rate             -->            
                            <!--    0x9              |  9/10 conv. code rate            -->            
                            <!--    0xA ~ 0xE        |  reserved for future use         -->            
                            <!--    0xF              |  no conv. Coding                 -->            
                            <FEC_inner> 0x3 </FEC_inner>            
                      </item>          
                      <!-- This section would take effective only when the "network_type" is set to DVB-T -->          
                      <item type="terrestrial">            
                            <!-- The center_frequency is a 32-bit field giving the centre frequency value in multiples of 10 Hz. The -->            
                            <!-- coding range is from minimum 10 Hz (0x00000001) up to a maximum of 42 949 672 950 Hz (0xFFFFFFFF). -->            
