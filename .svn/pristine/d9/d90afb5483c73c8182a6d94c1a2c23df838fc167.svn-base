    </transport>    

    <service>      
          <!-- Service ID (Program Number): [0x0000 ~ 0xFFFE] -->      
          <service_id> 0x0001 </service_id>      
    
          <!-- Service Type : 8 bits -->      
          <!-- download mode        |  Description                     -->      
          <!--    0x01              |  digital television service      -->      
          <!--    0x0C              |  data broadcast service          -->      
	      <service_type> 0x0C </service_type>      
	
	      <!-- SSU PMT PID : 13 bits, [0x0020 ~ 0x1FFE] -->      
          <ssu_pmt_pid> 0x0407 </ssu_pmt_pid>      
	      <pmt>        
                <!-- SSU Elementary Stream PID : [0x0020 ~ 0x1FFE] -->        
                <ssu_es_pid> 0x07D3 </ssu_es_pid>        
                <!-- A4 ETSI TS102006_V1_2 Data Broadcast Id Descriptor selector byte definition for system software update-->        
                <data_broadcast_id_descriptor>          
                      <!-- OUI : this is a 24-bit field containing an IEEE OUI (as described in IEEE 802-1990) of the organization providing  -->          
                      <!-- a system software update service on the transport-stream/service. DVB has defined OUI 0x00015A to signal that the  -->          
                      <!-- stream is from any OUI. -->          
                      <OUI id="pmt">0x16fc</OUI>          
                      <!-- update_type : 0x01 - Simple profile. Now we support this type ONLY. -->          
                      <update_type> 0x01 </update_type>          
                      <selector_byte>            
                            <!-- Manufacturer ID : [0x0001 ~ 0xFFFE] -->            
                            <manufacturer_id id="pmt">236</manufacturer_id>            
                            <!-- Hardware Version : [0x0001 ~ 0xFFFE] -->            
                            <hardware_version id="pmt">6</hardware_version>            
                            <!-- Reserved : fixed to 0x0000 -->            
                            <reserved> 0x0000 </reserved>            
                            <!-- Variant : [0x0001 ~ 0xFFFE] -->            
                            <variant id="pmt">236</variant>            
                            <!-- Sub-variant : [0x0000 ~ 0xFFFE] -->            
                            <sub_variant id="pmt">0</sub_variant>            
                            <!-- Download Sequence Number : [0x0001 ~ 0xFFFE] -->            
                            <download_sequence_number id="pmt">0x11</download_sequence_number>            
                            <!-- Trigger Control Byte : this is a one-byte control field containing the way in which this download would be able to be triggered. -->            
                            <!-- trigger control byte   |  auto        menu        EMM or front-panel  -->            
                            <!--    0x00                |  x           x               x               -->            
                            <!--    0x01                |  x           x               o               -->            
                            <!--    0x02                |  x           o               x               -->            
                            <!--    0x03                |  x           o               o               -->            
                            <!--    0x04                |  o           x               x               -->            
                            <!--    0x05                |  o           x               o               -->            
                            <!--    0x06                |  o           o               x               -->            
                            <!--    0x07                |  o           o               o               -->            
                            <trigger_control_byte id="pmt"> 0x07 </trigger_control_byte>            
                            <!-- Download Mode : this is a one-byte control field containing the way in which this download would be done. -->            
                            <!-- download mode        |  Description                     -->            
                            <!--    0x00              |  forced download                 -->            
                            <!--    0x01              |  background download             -->            
                            <!--    0x02              |  delayed download                -->            
                            <!--    0x03 ~ 0xFF       |  reserved for future use         -->            
                            <download_mode id="pmt"> 0x00 </download_mode>            
                      </selector_byte>          				
                </data_broadcast_id_descriptor>        
          </pmt>      
	
    </service>    

  </download>  
</LOADER_V5_HIGHLEVEL_DVT_CONFIG>