                                        <transposer_frequency> 0x00000555 </transposer_frequency>                
                                  </T2SubCellLoop>                   
                            </T2CellLoop>            
                            <T2CellLoop>              
                                  <!-- The cell_id is a 16-bit field  used to identify a cell . -->              
                                  <cell_id> 0x4321 </cell_id>                   
                                  <T2CellCentreFrequencyLoop>                
                                        <!-- The center_frequency is a 32-bit field giving the centre frequency value in multiples of 10 Hz. The -->                
                                        <!-- coding range is from minimum 10 Hz (0x00000001) up to a maximum of 42 949 672 950 Hz (0xFFFFFFFF). -->                
                                        <center_frequency> 0x00077618 </center_frequency>                
                                        <center_frequency> 0x00000619 </center_frequency>                
                                        <center_frequency> 0x00000620 </center_frequency>                
                                        <center_frequency> 0x00000621 </center_frequency>                
                                  </T2CellCentreFrequencyLoop>                                  
                                  <T2SubCellLoop>                
                                        <!-- The cell_id_extension is a 8-bit field  used to identify a sub-cell within a cell. -->                
                                        <cell_id_extension> 0x23 </cell_id_extension>                
                                        <transposer_frequency> 0x00000111 </transposer_frequency>                
                                        <cell_id_extension> 0x45 </cell_id_extension>                            
                                        <!-- The transposer_frequency is a 32-bit field. -->                
                                        <!-- it indicates the centre frequency that is used by a transposer in the sub-cell indicated.   -->                                
                                        <!-- It is encoded in the same way as the center_frequency field. -->                
