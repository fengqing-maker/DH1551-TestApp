<?xml version="1.0" encoding="utf-8"?><LOADER_V5_HIGHLEVEL_DVT_CONFIG>  
  <download id="irdeto">    

    <!-- The script is configured for a specific manufacturer -->    
    <manufacturer_info>      
          <name id="manu"> Castpal </name>      
          <!-- OUI : this is a 24-bit field containing an IEEE OUI (as described in IEEE 802-1990) of the organization providing  -->      
          <!-- a system software update service on the transport-stream/service. DVB has defined OUI 0x00015A to signal that the  -->      
          <!-- stream is from any OUI [0x0001 ~ 0xFFFFFF]. -->      	
          <OUI id="manu">0x0016FB</OUI>      
    </manufacturer_info>    

    <!-- This contains the .CD5 file for SSU download. -->    
    <cd5>      
          <!-- .CD5 file name -->      
          <name id="cd5">dvt.CD5</name>      
    </cd5>    

    <!-- This contains the global control options -->    
    <options>      
          <!-- EMM trigger : [on, off]. -->      
          <emm_trigger>off</emm_trigger>      
          <!-- the linkage descriptor lies in : [NIT, BAT]. -->      
          <linkage_descriptor_location>NIT</linkage_descriptor_location>      
          <!-- which transport stream will the SSU service be inserted : [actual, other]. -->      
          <ssu_location> actual </ssu_location>      
          <!-- linkage or multilinkage desriptor.[on, off] -->      
          <linkage_descriptor_multiple> off </linkage_descriptor_multiple>      	
    </options>    

    <emm>      
    
          <ird_emm>        
                <!-- EMM Filter Number : [0x10, 0x1F]. for CCA client, the value is fixed to 0x14.-->        
                <filter_number>0x12</filter_number>        
        
                <!-- Message Type : Decoder Control Message types. [force download, migration]-->        
                <message_type> force download </message_type>        
        
                <!-- Force Download -->            
                <item type="force download">          
                      <!-- Nothing to configure. -->          
                </item>        
        
                <!-- Migration -->            
                <item type="migration">          
                      <!-- New Variant : [0x0001 ~ 0xFFFE] -->          
                      <new_variant>322</new_variant>          
                      <!-- New Sub-variant : [0x0000 ~ 0xFFFE] -->          
                      <new_subvariant>0</new_subvariant>          
                </item>        
          </ird_emm>      
    
          <cat>        
                <ca_descriptor>          
                      <!-- CA system ID: 16 bits -->          
                      <ca_system_id>0x604</ca_system_id>          
                      <!-- EMM PID : 13 bits, [0x0020 ~ 0x1FFE] -->          
                      <ca_pid> 0x0123 </ca_pid>          
                </ca_descriptor>        
          </cat>      
    </emm>    


    <linkage_descriptor>      
          <!-- Variant : Fixed to 0x09 -->      
          <linkage_type> 0x09 </linkage_type>      
          <!-- OUI : this is a 24-bit field containing an IEEE OUI (as described in IEEE 802-1990) of the organization providing  -->      
          <!-- a system software update service on the transport-stream/service. DVB has defined OUI 0x00015A to signal that the  -->      
          <!-- stream is from any OUI. -->      
          <OUI id="linkage">0x0016FB</OUI>      
          <selector_byte>        
                <!-- Manufacturer ID : [0x0001 ~ 0xFFFE] -->        
                <manufacturer_id id="linkage">236</manufacturer_id>        
                <!-- Hardware Version : [0x00001 ~ 0xFFFE] -->        
                <hardware_version id="linkage">11</hardware_version>        
                <!-- Reserved : fixed to 0x0000 -->        
                <reserved id="linkage"> 0x0000 </reserved>        
                <!-- Variant : [0x0001 ~ 0xFFFE] -->        
                <variant id="linkage">322</variant>        
                <!-- Sub-variant : [0x0000 ~ 0xFFFE] -->        
                <sub_variant id="linkage">0</sub_variant>        
                <!-- Download Sequence Number : [0x0001 ~ 0xFFFE] -->        
                <download_sequence_number id="linkage">0x2</download_sequence_number>        
                <!-- Trigger Control Byte : this is a one-byte control field containing the way in which this download would be able to be triggered. -->        
                <!-- trigger control byte   |  auto        menu        EMM or front-panel  -->        
                <!--    0x00                |  x           x               x               -->        
                <!--    0x01                |  x           x               o               -->        
                <!--    0x02                |  x           o               x               -->        
                <!--    0x03                |  x           o               o               -->        
                <!--    0x04                |  o           x               x               -->        
                <!--    0x05                |  o           x               o               -->        
                <!--    0x06                |  o           o               x               -->        
                <!--    0x07                |  o           o               o               -->        
                <trigger_control_byte id="linkage"> 0x02 </trigger_control_byte>        
                <!-- Download Mode : this is a one-byte control field containing the way in which this download would be done. -->        
                <!-- download mode        |  Description                     -->        
                <!--    0x00              |  forced download                 -->        
                <!--    0x01              |  background download             -->        
                <!--    0x02              |  delayed download                -->        
                <!--    0x03 ~ 0xFF       |  reserved for future use         -->        
                <download_mode id="linkage"> 0x00 </download_mode>        
          </selector_byte>      
    </linkage_descriptor>    


    <multilinkage_descriptor>      
          <!-- Variant : Fixed to 0x09 -->      
          <linkage_type> 0x09 </linkage_type>      
          <!-- OUI : this is a 24-bit field containing an IEEE OUI (as described in IEEE 802-1990) of the organization providing  -->      
          <!-- a system software update service on the transport-stream/service. DVB has defined OUI 0x00015A to signal that the  -->      
          <!-- stream is from any OUI. -->      
          <OUI id="multi">0x0016FB</OUI>      
          <selector_byte>        
                <!-- Manufacturer ID : [0x0001 ~ 0xFFFE] -->        
                <manufacturer_id id="multi">236</manufacturer_id>        
                <!-- Hardware Version : [0x0001 ~ 0xFFFE] -->        
                <hardware_version id="multi">6</hardware_version>        
                <!-- Reserved : fixed to 0x0000 -->        
                <reserved id="multi"> 0x0000 </reserved>        
                <!-- Variant : [0x0001 ~ 0xFFFE] -->        
                <variant id="multi">236</variant>        
                <!-- Sub-variant : [0x0000 ~ 0xFFFE] -->        
                <sub_variant id="multi">0</sub_variant>        
                <!-- Download Sequence Number : [0x0001 ~ 0xFFFE] -->        
                <download_sequence_number id="multi">16</download_sequence_number>        
                <!-- Trigger Control Byte : this is a one-byte control field containing the way in which this download would be able to be triggered. -->        
                <!-- trigger control byte   |  auto        menu        EMM or front-panel  -->        
                <!--    0x00                |  x           x               x               -->        
                <!--    0x01                |  x           x               o               -->        
                <!--    0x02                |  x           o               x               -->        
                <!--    0x03                |  x           o               o               -->        
                <!--    0x04                |  o           x               x               -->        
                <!--    0x05                |  o           x               o               -->        
                <!--    0x06                |  o           o               x               -->        
                <!--    0x07                |  o           o               o               -->        
                <trigger_control_byte id="multi"> 0x07 </trigger_control_byte>        
                <!-- Download Mode : this is a one-byte control field containing the way in which this download would be done. -->        
                <!-- download mode        |  Description                     -->        
                <!--    0x00              |  forced download                 -->        
                <!--    0x01              |  background download             -->        
                <!--    0x02              |  delayed download                -->        
                <!--    0x03 ~ 0xFF       |  reserved for future use         -->        
                <download_mode id="multi"> 0x00 </download_mode>        
          </selector_byte>      
    </multilinkage_descriptor>    


    <transport>      
          <transport_stream attr="actual">        
                <!-- Transport Stream ID : [0x0000 ~ 0xFFFF] -->        
    	        <transport_stream_id> 0x0001 </transport_stream_id>        
    	        <!-- Original Network ID : [0x0000 ~ 0xFFFF] -->        
    	        <original_network_id> 0x5000 </original_network_id>        

    	        <delivery_system_descriptor>          
                      <!-- Network Type : [cable, satellite, terrestrial, satellite2 terrestrial2] -->          
                      <network_type>satellite</network_type>          
                      <!-- This section would take effective only when the "network_type" is set to DVB-C -->          
                      <item type="cable">            
                            <!-- The frequency is a 32-bit field giving the 4-bit BCD values specifying 8 characters of the frequency value. -->            
                            <!-- For the cable_delivery_system_descriptor, the frequency is coded in MHz, where the decimal occurs after the fourth -->            
                            <!-- character (e.g. 0470.0000 MHz) -->            
                            <frequency> 0x04700000 </frequency>            
                            <!-- The FEC_outer is a 4-bit field specifying the outer Forward Error Correction (FEC) scheme used according to the table below. -->            
                            <!-- FEC_outer bit 3210  |  Description                     -->            
                            <!--    0x0              |  not defined                     -->            
                            <!--    0x1              |  no outer FEC coding             -->            
                            <!--    0x2              |  RS(204/188)                     -->            
                            <!--    0x3 ~ 0xF        |  reserved for future use         -->            
                            <FEC_outer> 0x0 </FEC_outer>            
                            <!-- This is an 8-bit field. It specifies the modulation scheme used on a cable delivery system according to the table below. -->            
                            <!-- Modulation (hex)    |  Description                     -->            
                            <!--    0x00             |  not defined                     -->            
                            <!--    0x01             |  16-QAM                          -->            
                            <!--    0x02             |  32-QAM                          -->            
                            <!--    0x03             |  64-QAM                          -->            
                            <!--    0x04             |  128-QAM                         -->            
                            <!--    0x05             |  256-QAM                         -->            
                            <!--    0x06 ~ 0xFF      |  reserved for future use         -->            
                            <modulation> 0x03 </modulation>            
                            <!-- The symbol_rate is a 28-bit field giving the 4-bit BCD values specifying 7 characters of the symbol_rate -->            
                            <!-- in Msymbol/s where the decimal point occurs after the third character (e.g. 006.8750). -->            
                            <symbol_rate> 0x0068750 </symbol_rate>            
                            <!-- The FEC_inner is a 4-bit field specifying the inner FEC scheme used according to the table below. -->            
                            <!-- FEC_inner bit 3210  |  Description                     -->            
                            <!--    0x0              |  not defined                     -->            
                            <!--    0x1              |  1/2 conv. code rate             -->            
                            <!--    0x2              |  2/3 conv. code rate             -->            
                            <!--    0x3              |  3/4 conv. code rate             -->            
                            <!--    0x4              |  5/6 conv. code rate             -->            
                            <!--    0x5              |  7/8 conv. code rate             -->            
                            <!--    0x6              |  8/9 conv. code rate             -->            
                            <!--    0x7              |  3/5 conv. code rate             -->            
                            <!--    0x8              |  4/5 conv. code rate             -->            
                            <!--    0x9              |  9/10 conv. code rate            -->            
                            <!--    0xA ~ 0xE        |  reserved for future use         -->            
                            <!--    0xF              |  no conv. Coding                 -->            
                            <FEC_inner> 0x0 </FEC_inner>            
                      </item>          
                      <!-- This section would take effective only when the "network_type" is set to DVB-S -->          
                      <item type="satellite">            
                            <!-- The frequency is a 32-bit field giving the 4-bit BCD values specifying 8 characters of the frequency value. -->            
                            <!-- For the satellite_delivery_system_descriptor the frequency is coded in GHz, where the decimal point occurs after the -->            
                            <!-- third character (e.g. 010.82000 GHz). -->            
                            <frequency> 0x01070000 </frequency>            
                            <!-- The orbital_position is a 16-bit field giving the 4-bit BCD values specifying 4 characters of the -->            
                            <!-- orbital position in degrees where the decimal point occurs after the third character (e.g. 156.0¡᧮ -->            
                            <orbital_position> 0x1560 </orbital_position>            
                            <!-- The west_east_flag is a 1-bit field indicating if the satellite position is in the western or eastern part of -->            
                            <!-- the orbit. A value "0" indicates the western position and a value "1" indicates the eastern position. -->            
                            <west_east_flag> 1 </west_east_flag>            
                            <!-- The polarization is a 2-bit field specifying the polarization of the transmitted signal. The first bit defines -->            
                            <!-- whether the polarization is linear or circular. See the table below. -->            
                            <!-- Polarization        |  Description                     -->            
                            <!--    0x0              |  linear - horizontal             -->            
                            <!--    0x1              |  linear - vertical               -->            
                            <!--    0x2              |  Circular - left                 -->            
                            <!--    0x3              |  Circular - right                -->            
                            <polarization> 0x0 </polarization>            
                            <!-- This 2 bit field specifies the modulation system factor used in DVB-S2. -->            
                            <!-- roll_off            |  Description                     -->            
                            <!--    0x0              |  ¦�= 0,35                        -->            
                            <!--    0x1              |  ¦�= 0,25                        -->            
                            <!--    0x2              |  ¦�= 0,20                        -->            
                            <!--    0x3              |  reserved                        -->            
                            <roll_off> 0x0 </roll_off>            
                            <!-- This is a 1-bit field. It specifies the broadcast scheme used on a satellite delivery system according to the table below. -->            
                            <!-- modulation_system   |  Description                     -->            
                            <!--    0                |  DVB-S                           -->            
                            <!--    1                |  DVB-S2                          -->            	
                            <modulation_system> 0 </modulation_system>            
                            <!-- This is a 2-bit field. It specifies the modulation scheme used on a satellite delivery system according to the table below. -->            
                            <!-- modulation type     |  Description                     -->            
                            <!--    0x0              |  Auto                            -->            
                            <!--    0x1              |  QPSK                            -->            
                            <!--    0x2              |  8PSK                            -->            
                            <!--    0x3              |  16-QAM (n/a for DVB-S2)         -->            
                            <modulation_type> 0x1 </modulation_type>            
                            <!-- The symbol_rate is a 28-bit field giving the 4-bit BCD values specifying 7 characters of the symbol_rate -->            
                            <!-- in Msymbol/s where the decimal point occurs after the third character (e.g. 026.0000). -->            
                            <symbol_rate> 0x0275000 </symbol_rate>            
                            <!-- The FEC_inner is a 4-bit field specifying the inner FEC scheme used according to the table below. -->            
                            <!-- FEC_inner bit 3210  |  Description                     -->            
                            <!--    0x0              |  not defined                     -->            
                            <!--    0x1              |  1/2 conv. code rate             -->            
                            <!--    0x2              |  2/3 conv. code rate             -->            
                            <!--    0x3              |  3/4 conv. code rate             -->            
                            <!--    0x4              |  5/6 conv. code rate             -->            
                            <!--    0x5              |  7/8 conv. code rate             -->            
                            <!--    0x6              |  8/9 conv. code rate             -->            
                            <!--    0x7              |  3/5 conv. code rate             -->            
                            <!--    0x8              |  4/5 conv. code rate             -->            
                            <!--    0x9              |  9/10 conv. code rate            -->            
                            <!--    0xA ~ 0xE        |  reserved for future use         -->            
                            <!--    0xF              |  no conv. Coding                 -->            
                            <FEC_inner> 0x3 </FEC_inner>            
                      </item>          
                      <!-- This section would take effective only when the "network_type" is set to DVB-T -->          
                      <item type="terrestrial">            
                            <!-- The center_frequency is a 32-bit field giving the centre frequency value in multiples of 10 Hz. The -->            
                            <!-- coding range is from minimum 10 Hz (0x00000001) up to a maximum of 42 949 672 950 Hz (0xFFFFFFFF). -->            
                            <center_frequency> 0x00000618 </center_frequency>            
                            <!-- This is a 3-bit field specifying the bandwidth in use. See the table below. -->            
                            <!-- bandwidth           |  Description                     -->            
                            <!--    0x0              |  8 MHz                           -->            
                            <!--    0x1              |  7 MHz                           -->            
                            <!--    0x2              |  6 MHz                           -->            
                            <!--    0x3              |  5 MHz                           -->            
                            <!--    0x4 ~ 0x7        |  reserved for future use         -->            
                            <bandwidth> 0x0 </bandwidth>            
                            <!-- This is a 2-bit field. It specifies the constellation pattern used on a terrestrial delivery system according to the table below. -->            
                            <!-- Constellation       |  Constellation characteristics   -->            
                            <!--    0x0              |  Auto                            -->            
                            <!--    0x1              |  QPSK                            -->            
                            <!--    0x2              |  8PSK                            -->            
                            <!--    0x3              |  16-QAM (n/a for DVB-S2)         -->            
                            <constellation> 0x0 </constellation>            
                            <!-- The hierarchy_information specifies whether the transmission is hierarchical and, if so, what -->            
                            <!-- the ¦�value is. Also, the use of in-depth interleaver is indicated. When the transmission_mode indicates the use of 8k -->            
                            <!-- mode, only the native interleaver shall be signalled. -->            
                            <!-- Hierarchy_information |  ¦�value                                 -->            
                            <!--    0x0                |  non-hierarchical, native interleaver    -->            
                            <!--    0x1                |  ¦�= 1, native interleaver               -->            
                            <!--    0x2                |  ¦�= 2, native interleaver               -->            
                            <!--    0x3                |  ¦�= 4, native interleaver               -->            
                            <!--    0x4                |  non-hierarchical, in-depth interleaver  -->            
                            <!--    0x5                |  ¦�= 1, in-depth interleaver             -->            
                            <!--    0x6                |  ¦�= 2, in-depth interleaver             -->            
                            <!--    0x7                |  ¦�= 4, in-depth interleaver             -->            
                            <hierarchy_information> 0x0 </hierarchy_information>            
                            <!-- The code_rate is a 3-bit field specifying the inner FEC scheme used according to the table below. -->            
                            <!-- Non-hierarchical channel coding and modulation requires signalling of one code rate. In this case, 3 bits specifying code_rate -->            
                            <!-- according to the table below are followed by another 3 bits of value "000". -->            
                            <!-- code_rate           |  Description                     -->            
                            <!--    0x0              |  1/2                             -->            
                            <!--    0x1              |  2/3                             -->            
                            <!--    0x2              |  3/4                             -->            
                            <!--    0x3              |  5/6                             -->            
                            <!--    0x4              |  7/8                             -->            
                            <!--    0x5 ~ 0x7        |  reserved for future use         -->            
                            <code_rate_HP_stream> 0x0 </code_rate_HP_stream>            
                            <code_rate_LP_stream> 0x0 </code_rate_LP_stream>            
                            <!-- The guard_interval is a 2-bit field specifying the guard interval according to the table below. -->            
                            <!-- guard_interval      |  Guard interval values           -->            
                            <!--    0x0              |  1/32                            -->            
                            <!--    0x1              |  1/16                            -->            
                            <!--    0x2              |  1/8                             -->            
                            <!--    0x3              |  1/4                             -->            
                            <guard_interval> 0x0 </guard_interval>            
                            <!-- This 2-bit field indicates the number of carriers in an OFDM frame. -->            
                            <!-- guard_interval      |  Description                     -->            
                            <!--    0x0              |  2k mode                         -->            
                            <!--    0x1              |  8k mode                         -->            
                            <!--    0x2              |  4k mode                         -->            
                            <!--    0x3              |  reserved for future use         -->            
                            <transmission_mode> 0x0 </transmission_mode>            
                            <!-- This 1-bit flag indicates whether other frequencies are in use. The value "0" indicates that no -->            
                            <!-- other frequency is in use, "1" indicates that one or more other frequencies are in use. -->            
                            <other_frequency_flag> 0 </other_frequency_flag>            
                      </item>          
                      <!-- This section would take effective only when the "network_type" is set to DVB-T2 -->          
                      <item type="terrestrial2">                      
                            <!-- The plp_id is a 8-bit field. -->            
                            <!-- This 8-bit field uniquely identifies a data PLP within a T2 System, within a T2 Network.  -->            
                            <!-- The term is defined in EN 302 755 [47]. -->            
                            <plp_id> 0x01 </plp_id>            
                            <!-- The T2_system_id is a 16-bit field. -->            
                            <!-- This 16-bit field uniquely identifies a T2 system within a T2 network.  -->            
                            <!-- The term is defined in EN 302 755 [47]. -->            
                            <T2_system_id> 0x0102 </T2_system_id>            
                            <!-- The SISO/MISO is a 2-bit field. -->            
                            <!-- This 2-bit field indicates the SISO/MISO mode  -->            
                            <!--    00              |  SISO                           -->            
                            <!--    01              |  MISO                           -->            
                            <!--    10              |  reserved for future use        -->            
                            <!--    11              |  reserved for future use        -->            
                            <SISO_MISO> 0x01 </SISO_MISO>                
                            <!-- The bandwidth is a 4-bit field. -->            
                            <!-- This 2-bit field indicates the bandwidth  -->            
                            <!--    0000              |  8 MHz                       -->            
                            <!--    0001              |  7 MHz                       -->            
                            <!--    0010              |  6 MHz                       -->            
                            <!--    0011              |  5 MHz                       -->            
                            <!--    0100              |  10 MHz                      -->            
                            <!--    0101              |  1,712 MHz                   -->            
                            <!--    0110 to 1111      |  reserved for future use     -->                             
                            <bandwidth> 0x0000 </bandwidth>                                   
                            <!-- This 3-bit field indicates the guard interval .      -->            
                            <!-- guard_interval      |  Description                 -->            
                            <!--    000              |  1/32                        -->            
                            <!--    001              |  1/16                        -->            
                            <!--    010              |  1/8                         -->            
                            <!--    011              |  1/4                         -->            
                            <!--    100              |  1/128                       -->            
                            <!--    101              |  19/128                      -->            
                            <!--    110              |  19/256                      -->            
                            <!--    111              |  reserved for future use     -->                                
                            <guard_interval> 0x000 </guard_interval>                            
                            <!-- This 3-bit field indicates the FFT size of the signals transmitted within the associated cell. -->            
                            <!-- transmission_mode     |  Description                     -->            
                            <!--    0x000              |  2k mode                         -->            
                            <!--    0x001              |  8k mode                         -->            
                            <!--    0x010              |  4k mode                         -->            
                            <!--    0x011              |  8k mode                         -->            
                            <!--    0x100              |  16K mode                        -->                            
                            <!--    0x101              |  32k mode                        -->                            
                            <!--    0x110-111          |  reserved for future use         -->                                       
                            <transmission_mode> 0x000 </transmission_mode>                      
                            <!-- This 1-bit flag indicates whether other frequencies (non-TFS case) or other groups of frequencies (TFS case) are in use. -->            
                            <!-- The value 0 (zero) indicates that the set of frequencies  -->            
                            <!-- (non-TFS case) or the set of groups of frequencies (TFS case) included in the descriptor is complete, -->            
                            <!-- the value 1 (one) indicates that the set is incomplete. -->            
                            <other_frequency_flag> 0 </other_frequency_flag>                        
                            <!-- The tfs_flag is a 1-bit field. -->            
                            <!-- This 1-bit flag indicates whether a TFS arrangement is in place or not.  -->            
                            <!--    0x0              |  No TFS arrangement in pl                          -->            
                            <!--    0x1              |  TFS arrangement in place                          -->            
                            <tfs_flag> 0x01 </tfs_flag>            
                            <T2CellLoop>              
                                  <!-- The cell_id is a 16-bit field  used to identify a cell . -->              
                                  <cell_id> 0x1234 </cell_id>                   
                                  <T2CellCentreFrequencyLoop>                
                                        <!-- The center_frequency is a 32-bit field giving the centre frequency value in multiples of 10 Hz. The -->                
                                        <!-- coding range is from minimum 10 Hz (0x00000001) up to a maximum of 42 949 672 950 Hz (0xFFFFFFFF). -->                
                                        <center_frequency> 0x000333 </center_frequency>                
                                        <center_frequency> 0x00000444 </center_frequency>                
                                        <center_frequency> 0x00000555 </center_frequency>                
                                        <center_frequency> 0x00000666 </center_frequency>                
                                  </T2CellCentreFrequencyLoop>                                  
                                  <T2SubCellLoop>                
                                        <!-- The cell_id_extension is a 8-bit field  used to identify a sub-cell within a cell. -->                
                                        <cell_id_extension> 0x67 </cell_id_extension>                
                                        <transposer_frequency> 0x00000222 </transposer_frequency>                
                                        <cell_id_extension> 0x78 </cell_id_extension>                            
                                        <!-- The transposer_frequency is a 32-bit field. -->                
                                        <!-- it indicates the centre frequency that is used by a transposer in the sub-cell indicated.   -->                                
                                        <!-- It is encoded in the same way as the center_frequency field. -->                
                                        <transposer_frequency> 0x00000555 </transposer_frequency>                
                                  </T2SubCellLoop>                   
                            </T2CellLoop>            
                            <T2CellLoop>              
                                  <!-- The cell_id is a 16-bit field  used to identify a cell . -->              
                                  <cell_id> 0x4321 </cell_id>                   
                                  <T2CellCentreFrequencyLoop>                
                                        <!-- The center_frequency is a 32-bit field giving the centre frequency value in multiples of 10 Hz. The -->                
                                        <!-- coding range is from minimum 10 Hz (0x00000001) up to a maximum of 42 949 672 950 Hz (0xFFFFFFFF). -->                
                                        <center_frequency> 0x00077618 </center_frequency>                
                                        <center_frequency> 0x00000619 </center_frequency>                
                                        <center_frequency> 0x00000620 </center_frequency>                
                                        <center_frequency> 0x00000621 </center_frequency>                
                                  </T2CellCentreFrequencyLoop>                                  
                                  <T2SubCellLoop>                
                                        <!-- The cell_id_extension is a 8-bit field  used to identify a sub-cell within a cell. -->                
                                        <cell_id_extension> 0x23 </cell_id_extension>                
                                        <transposer_frequency> 0x00000111 </transposer_frequency>                
                                        <cell_id_extension> 0x45 </cell_id_extension>                            
                                        <!-- The transposer_frequency is a 32-bit field. -->                
                                        <!-- it indicates the centre frequency that is used by a transposer in the sub-cell indicated.   -->                                
                                        <!-- It is encoded in the same way as the center_frequency field. -->                
                                        <transposer_frequency> 0x00000618 </transposer_frequency>                
                                  </T2SubCellLoop>                   
                            </T2CellLoop>            
                       </item>          
                      <!-- This section would take effective only when the "network_type" is set to DVB-S2 -->                       
                      <item type="satellite2">            
                            <!-- The scrambling_sequence_selector is a 1-bit field. -->            
                            <!-- A value 0 specifies that the default DVB-S2 physical layer scrambling sequence of index n = 0 is used.  -->            
                            <!-- A value 1 means that the default scrambling sequence is not used and -->            
                            <!-- that the sequence index n will be conveyed by the scrambling_sequence_index field. -->            
                            <scrambling_sequence_selector> 0x1 </scrambling_sequence_selector>            
                            <!-- The multiple_input_stream_flag is a 1-bit field indicating whether single or multiple -->            
                            <!-- transport streams are conveyed.. -->            
