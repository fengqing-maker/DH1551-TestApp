                            <!--    0011            |  5 MHz                       -->            
                            <!--    0100            |  10 MHz                      -->            
                            <!--    0101            |  1,712 MHz                   -->            
                            <!--    0110 to 1111    |  reserved for future use     -->                          
                            <bandwidth> 0x0000 </bandwidth>                                   
                            <!-- This 3-bit field indicates the guard interval .      -->            
                            <!-- guard_interval        |  Description                 -->            
                            <!--    000              |  1/32                        -->            
                            <!--    001              |  1/16                        -->            
                            <!--    010              |  1/8                         -->            
                            <!--    011              |  1/4                         -->            
                            <!--    100              |  1/128                       -->            
                            <!--    101              |  19/128                      -->            
                            <!--    110              |  19/256                      -->            
                            <!--    111              |  reserved for future use     -->                                
                            <guard_interval> 0x000 </guard_interval>                            
                            <!-- This 3-bit field indicates the FFT size of the signals transmitted within the associated cell. -->            
                            <!-- transmission_mode   |  Description                     -->            
                            <!--    000              |  2k mode                         -->            
                            <!--    001              |  8k mode                         -->            
                            <!--    010              |  4k mode                         -->            
                            <!--    011              |  8k mode                         -->            
                            <!--    100              |  16K mode                        -->                            
