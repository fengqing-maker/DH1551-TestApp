                            <other_frequency_flag> 0 </other_frequency_flag>            
                      </item>          
                      <!-- This section would take effective only when the "network_type" is set to DVB-T2 -->          
                      <item type="terrestrial2">                      
                            <!-- The plp_id is a 8-bit field. -->            
